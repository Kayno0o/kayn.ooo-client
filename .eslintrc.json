{
  "extends": [
    "@antfu",
    "plugin:jsx-a11y/recommended"
  ],
  "plugins": [
    "jsx-a11y"
  ],
  "rules": {
    "@next/next/no-img-element": "off",
    "max-lines": [
      "warn",
      {
        "max": 300,
        "skipBlankLines": true,
        "skipComments": true
      }
    ],
    "no-unused-vars": "off",
    "sort-keys": "warn",
    "sort-vars": "warn"
  }
}
